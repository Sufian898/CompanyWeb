{
  "version": 2,
  "buildCommand": "echo 'Backend-only deployment - no build needed'",
  "outputDirectory": ".",
  "functions": {
    "api/index.js": {
      "runtime": "nodejs18.x",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
